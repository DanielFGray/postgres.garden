{
  "name": "postgres-garden",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "projectName": "Postgres Garden",
  "fromEmail": "noreply@postgres.garden",
  "legalText": "Postgres Garden - Open Source PostgreSQL Development Environment",
  "scripts": {
    "dev:client": "vite dev",
    "dev": "mprocs 'bun dev:server' 'bun db:watch' 'bun worker' 'bun test:watch' 'bun typecheck:watch' 'bun cypress:browser' 'bun build:renderer --watch' 'bun build:webviews --watch'",
    "dev:server": "bun --hot server/index.ts 2>&1 | tee -a ./server.log",
    "predev": "run-s db:ensure",
    "prebuild": "rm -fr dist",
    "build:server": "NODE_ENV=production bun scripts/serverBuild.ts",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "build:webviews": "vite build --config vite.webviews.config.ts",
    "build:client": "vite build",
    "build:copy-webviews": "mkdir -p dist/src/features/playground && cp -r src/features/playground/webview-dist dist/src/features/playground/",
    "build": "run-s build:server build:webviews build:renderer build:client build:copy-webviews",
    "start": "NODE_ENV=production bun dist/server.js",
    "migrate": "bun -b graphile-migrate migrate",
    "start:prod": "bun run migrate && bun run start",
    "init:prod": "NOCONFIRM=1 bun scripts/dbSetup.ts && bun -b graphile-migrate reset --erase",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck": "tsc --noEmit",
    "start:debugServer": "bun src/debugServer.ts",
    "cypress:chromium": "cypress open --e2e --browser=chromium",
    "cypress:firefox": "cypress open --e2e --browser=firefox",
    "cypress:browser": "cypress open --e2e",
    "cypress:headless": "cypress run",
    "test:unit": "bun test server/",
    "test:watch": "bun test --watch server/",
    "test:e2e": "run-s cypress:headless",
    "test": "run-s test:unit test:e2e",
    "worker": "cd worker && bun --env-file=../.env graphile-worker",
    "worker:schema": "cd worker && bun --env-file=../.env graphile-worker --schema-only",
    "env:init": "bun scripts/generateEnvFile.ts",
    "gm": "bun -b graphile-migrate",
    "db:init": "bun scripts/dbSetup.ts",
    "db:ensure": "bun scripts/dbEnsure.ts",
    "db:up": "docker-compose up -d db",
    "db:latest": "bun -b graphile-migrate watch --once",
    "db:watch": "bun -b graphile-migrate watch",
    "db:reset": "bun -b graphile-migrate reset --erase",
    "db:stop": "docker-compose stop db",
    "db:exec": "docker-compose exec db",
    "db:types": "kysely-codegen --dialect postgres --exclude-pattern 'graphile*.*' --out-file ./generated/db.ts",
    "db:setup": "run-s db:up db:init db:reset db:latest",
    "init": "NOCONFIRM=1 run-s env:init db:setup worker:schema"
  },
